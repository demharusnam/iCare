<body>
    #extend("base"):
    #export("content"):
    <h1 class = "title">Visiting?</h1>
    <!-- Start: Single Page Contact Us Form -->
    <div style="opacity: 1;background: #ffffff;">
        <div class="container">
            <div class="row">
                <div class="col-md-6" style="background: rgb(255,255,255);">
                    <form class = "visitorForm">
                        <div class="form-group">
                            <input type="text" style="width: 330px;height: 39px;color: #376d4b;" required="" placeholder="First Name">
                        </div>
                        
                        <div class="form-group">
                            <input type="text" placeholder="Last Name" style="width: 330px;height: 38px;color: #376d4b;">
                        </div>
                        
                        <div class="form-group">
                            <input type="text" placeholder="Patients Visiting ID" style="width: 330px;height: 38px;color: #376d4b;" required="">
                        </div>
                        
                        <div class="form-group">
                            <input type = "text" style="width: 330px;color: #376d4b;" placeholder="Message" required="">
                        </div>
                        
                        <div class="form-group">
                            <button class="btn btn-info" type="submit" style="width: 330px;background: #376d4b;">Submit</button>
                        </div>
                    </form>
                </div><!-- Start: con -->
                <div class="col-md-6" style="padding-left: 20px;padding-right: 20px;background: rgb(255,255,255);">
                    <img src="/images/Screen%20Shot%202021-04-07%20at%2011.55.32%20AM.png?h=e70a2b28437db5a9ada000fbaccfec18" style="width: 398px;margin: 46px;"/>
                </div><!-- End: con -->
            </div>
        </div>
